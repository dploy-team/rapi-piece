{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "piece": {
      "projectType": "library",
      "root": "projects/piece",
      "sourceRoot": "projects/piece/src",
      "prefix": "piece",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/piece/tsconfig.lib.json",
            "project": "projects/piece/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "piece"
}
